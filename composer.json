{
    "name": "wonderwp/asset",
    "type": "library",
    "description": "WonderWp Asset Component",
    "keywords": ["log"],
    "homepage": "https://wonderwp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Desvaux",
            "homepage": "http://jdmweb.com"
        },
        {
            "name": "WonderWp Community",
            "homepage": "https://wonderwp.com/contributors"
        }
    ],
    "require": {
        "php": "~7",
        "wonderwp/logging": "^1.0",
        "wonderwp/service": "^1.0",
        "symfony/asset": "5.4.x-dev"
    },
    "autoload": {
        "psr-4": {"WonderWp\\Component\\Asset\\": "src", "WonderWp\\Component\\Asset\\Tests\\": "tests"},
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    }
}
